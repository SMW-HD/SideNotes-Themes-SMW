{
    "$schema": "https://www.apptorium.com/schemas/sntheme.json",
    "light": {
        "folder": {
            "notes": {
                "clean": {
                    "code": {
                        "textColor": "#00000077"
                    },
                    "codeBlock": {
                        "textColor": "#00000077"
                    },
                    "quote": {
                        "textColor": "#00000077"
                    }
                },
                "colors": [
                    {
                        "backgroundColor": "#282A36",
                        "backgroundGradient": {
                            "start": "#282A36",
                            "end": "#181920"
                        }
                    },
                    {
                        "backgroundColor": "#F8F8F2",
                        "backgroundGradient": {
                            "start": "#F8F8F2",
                            "end": "#C6C6C2"
                        }
                    },
                    {
                        "backgroundColor": "#44475A",
                        "backgroundGradient": {
                            "start": "#44475A",
                            "end": "#363948"
                        }
                    }
                ]
            }
        },
        "search": {
            "searchField": {
                "backgroundColor": "#ffffffdd",
                "textColor": "#000000cc",
                "shadow": {
                    "blurRadius": 0,
                    "offsetY": 0
                }
            }
        }
    },
    "dark": {
        "folder": {
            "notes": {
                "clean": {
                    "code": {
                        "textColor": "#ffffff99"
                    },
                    "codeBlock": {
                        "textColor": "#ffffff99"
                    },
                    "quote": {
                        "textColor": "#ffffff99"
                    }
                },
                "colors": [
                    {
                        "backgroundColor": "#753636",
                        "backgroundGradient": {
                            "start": "#753636",
                            "end": "#612D2D"
                        },
                        "textColor": "#ffffff"
                    },
                    {
                        "backgroundColor": "#317E55",
                        "backgroundGradient": {
                            "start": "#317E55",
                            "end": "#236A44"
                        },
                        "textColor": "#ffffff",
                        "linkColor": "#7fb2ff"
                    },
                    {
                        "backgroundColor": "#3E5097",
                        "backgroundGradient": {
                            "start": "#3E5097",
