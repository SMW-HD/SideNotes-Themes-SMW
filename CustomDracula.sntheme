                    },
                    {
                        "backgroundColor": "#535353",
                        "backgroundGradient": {
                            "start": "#535353",
                            "end": "#454545"
                        },
                        "textColor": "#ffffff"
                    }
                ]
            }
        },
        "search": {
            "searchField": {
                "backgroundColor": "#00000033",
                "shadow": {
                    "blurRadius": 0,
                    "offsetY": 0
                }
            }
        }
    }
